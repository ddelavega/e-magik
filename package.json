{
  "name": "backoffice",
  "version": "0.0.14",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "adm": "ng serve --project bo-administrator --port 4300 -o",
    "org": "ng serve --project bo-organization --port 4400 -o",
    "build": "ng build",
    "build:adm": "ng build --project bo-administrator --configuration production",
    "build:org": "ng build --project bo-organization --configuration production",
    "build:adm-dev": "ng build --project bo-administrator --configuration=dev",
    "build:org-dev": "ng build --project bo-organization --configuration=dev",
    "ngAdm-c": "ng g c --project bo-administrator",
    "ngAdm-s": "ng g s --project bo-administrator",
    "ngAdm-m": "ng g m --project bo-administrator",
    "ngOrg-c": "ng g c --project bo-organization",
    "ngOrg-s": "ng g s --project bo-organization",
    "ngOrg-m": "ng g m --project bo-organization",
    "kill": "npx npkill",
    "test": "ng test",
    "lint": "ng lint",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/cdk": "^12.0.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/material": "^12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "ng-sidebar": "^9.4.2",
    "postcss": "^8.2.15",
    "rxjs": "~6.6.0",
    "sass": "^1.32.12",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.2.4"
  }
}
