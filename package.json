    "ng": "ng",
    "start": "ng serve",
    "adm": "ng serve --project e-admin --port 4300 -o",
    "org": "ng serve --project e-pages --port 4400 -o",
    "build": "ng build",
    "build:adm": "ng build --project e-admin --configuration production",
    "build:org": "ng build --project e-pages --configuration production",
    "build:adm-dev": "ng build --project e-admin --configuration=dev",
    "build:org-dev": "ng build --project e-pages --configuration=dev",
    "ngAdm-c": "ng g c --project e-admin",
    "ngAdm-s": "ng g s --project e-admin",
    "ngAdm-m": "ng g m --project e-admin",
    "ngOrg-c": "ng g c --project e-pages",
    "ngOrg-s": "ng g s --project e-pages",
    "ngOrg-m": "ng g m --project e-pages",
    "kill": "npx npkill",
    "test": "ng test",
    "lint": "ng lint",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/cdk": "^12.0.0",
